package String;

public class TestSubString {
	public static void main(String args[]){
		String k = "hkt";
		String s = "abc hkt cde";
		int i = s.indexOf(k);
		String bs = s.substring(0,i);
		String ks = s.substring(i, i+k.length());
		String es = s.substring(i+k.length());
		System.out.println(s);
		System.out.println(bs);
		System.out.println(ks);
		System.out.println(es);
	}
}
